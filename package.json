{
  "name": "portfolio-v3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "gen": "graphql-codegen --require dotenv/config --config codegen.yml dotenv_config_path=.env.local",
    "dev": "npm run gen && next dev",
    "build": "npm run gen && next build",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:staged": "lint-staged",
    "upgrade-deps": "ncu -u -i",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "npm run build && start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "npm run build && start-server-and-test start http://localhost:3000 cypress:headless",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@graphcms/rich-text-react-renderer": "^0.6.1",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-aspect-ratio": "^1.0.0",
    "@radix-ui/react-scroll-area": "^1.0.0",
    "@radix-ui/react-separator": "^1.0.0",
    "@radix-ui/react-visually-hidden": "^1.0.0",
    "@stitches/react": "^1.2.8",
    "@xstate/react": "^3.0.1",
    "axios": "^0.27.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^6.5.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "gray-matter": "^4.0.3",
    "next": "^12.3.1",
    "next-mdx-remote": "^4.1.0",
    "next-seo": "^5.5.0",
    "next-sitemap": "^3.1.23",
    "next-themes": "^0.2.1",
    "next-urql": "^4.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-media": "^1.10.0",
    "react-use-measure": "^2.1.1",
    "reading-time": "^1.5.0",
    "rxjs": "^7.5.7",
    "sharp": "^0.31.1",
    "urql": "^3.0.3",
    "xstate": "^4.33.6",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@graphcms/rich-text-types": "^0.5.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-urql": "^3.7.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/node": "^18.7.23",
    "@types/react": "17.0.41",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cross-env": "^7.0.3",
    "cypress": "^10.9.0",
    "dotenv": "^16.0.3",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "file-loader": "^6.2.0",
    "globby": "^13.1.2",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^16.3.4",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "react-test-renderer": "^17.0.2",
    "rewire": "^6.0.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0"
  }
}
