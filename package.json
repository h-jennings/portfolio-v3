{
  "name": "portfolio-v3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "gen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "dev": "npm run gen && next dev",
    "build": "npm run gen && next build",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:staged": "lint-staged",
    "upgrade-deps": "ncu -u -i",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "npm run build && start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "npm run build && start-server-and-test start http://localhost:3000 cypress:headless",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@graphcms/rich-text-react-renderer": "^0.6.1",
    "@graphcms/utils": "^1.2.0",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-aspect-ratio": "^1.0.1",
    "@radix-ui/react-scroll-area": "^1.0.2",
    "@radix-ui/react-separator": "^1.0.1",
    "@radix-ui/react-visually-hidden": "^1.0.1",
    "@tanstack/react-query": "^4.22.4",
    "@tanstack/react-query-devtools": "^4.22.4",
    "@vanilla-extract/css": "^1.9.3",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.3.0",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "@vercel/og": "^0.0.20",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^1.2.1",
    "contentlayer": "^0.3.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^8.5.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "next": "^13.1.5",
    "next-contentlayer": "^0.3.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.47",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-media": "^1.10.0",
    "react-use-measure": "^2.1.1",
    "reading-time": "^1.5.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@graphcms/rich-text-types": "^0.5.0",
    "@graphql-codegen/add": "^3.2.3",
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/schema-ast": "^2.6.1",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-document-nodes": "^2.3.12",
    "@graphql-codegen/typescript-operations": "^2.5.12",
    "@next/bundle-analyzer": "^13.1.5",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/node": "^18.11.18",
    "@types/react": "18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.3.0",
    "dotenv": "^16.0.3",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-check-updates": "^16.6.3",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "start-server-and-test": "^1.15.3",
    "typescript": "^4.9.4"
  }
}
