{
  "name": "portfolio-v3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "gen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env.local",
    "dev": "pnpm run gen && next dev",
    "build": "pnpm run gen && next build && next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:staged": "lint-staged",
    "upgrade-deps": "ncu -u -i",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "pnpm run build && start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "pnpm run build && start-server-and-test start http://localhost:3000 cypress:headless",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@giscus/react": "^2.2.8",
    "@graphcms/rich-text-react-renderer": "^0.6.1",
    "@hygraph/utils": "^1.2.1",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-aspect-ratio": "^1.0.1",
    "@radix-ui/react-scroll-area": "^1.0.2",
    "@radix-ui/react-separator": "^1.0.1",
    "@radix-ui/react-visually-hidden": "^1.0.1",
    "@tanstack/react-query": "^4.24.10",
    "@tanstack/react-query-devtools": "^4.24.14",
    "@vanilla-extract/css": "^1.9.5",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.3.0",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "@vercel/analytics": "^0.1.11",
    "@vercel/og": "^0.3.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^1.2.1",
    "contentlayer": "^0.3.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.0.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "graphql-tag": "^2.12.6",
    "next": "^13.2.3",
    "next-contentlayer": "^0.3.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^4.0.2",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-media": "^1.10.0",
    "react-use-measure": "^2.1.1",
    "react-wrap-balancer": "^0.4.0",
    "reading-time": "^1.5.0",
    "rehype-pretty-code": "^0.9.4",
    "sharp": "^0.31.3",
    "shiki": "^0.14.1",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@graphcms/rich-text-types": "^0.5.0",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/schema-ast": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-document-nodes": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@next/bundle-analyzer": "^13.2.3",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/node": "^18.14.6",
    "@types/react": "18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "dotenv": "^16.0.3",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
